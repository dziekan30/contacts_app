{
  id: contact.id,
  first_name: contact.first_name,
  last_name: contact.last_name,
  email: contact.email,
  phone_number: contact.phone_number,
  user: (render partial: "api/users/user.json.jb", locals: {user: contact.user}),
  groups: contact.groups.map { |group| render partial: "api/groups/group.jason.jb". locals: {group: group}},
  categories:  contact.categories.map {|category| category.first_name }

}